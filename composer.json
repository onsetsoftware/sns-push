{
    "name": "acurrieclark/sns-push",
    "type": "library",
    "description": "API Wrapper for Amazon SNS PHP SDK",
    "keywords": [
        "amazon",
        "aws",
        "sns",
        "api"
    ],
    "homepage": "https://github.com/ReduGroup/sns-push",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SNSPush\\": "src/",
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Alex Currie-Clark",
            "email": "alex@onsetsoftware.io",
            "role": "Developer"
        },
        {
            "name": "Michael Forcer",
            "email": "tech@redu.co.uk",
            "role": "Developer"
        },
        {
            "name": "Jonathon Henderson",
            "email": "tech@redu.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "aws/aws-sdk-php": "^3.36",
        "aws/aws-php-sns-message-validator": "^1.4",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "doctrine/instantiator": "^1.0.5"
    }
}
